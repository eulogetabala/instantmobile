{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_USE_LOCALHOST": "false",
        "EXPO_PUBLIC_API_URL": "https://instant-backend-1.onrender.com/api"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_USE_LOCALHOST": "false",
        "EXPO_PUBLIC_API_URL": "https://instant-backend-1.onrender.com/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
