+  ~WorkletsSystraceSection() {
+    os_signpost_interval_end(
+        worklets::instrumentsLogHandle,
+        signpostID_,
+        "Worklets",
+        "%s end",
+        name_.data());
+  }
+
+ private:
+  os_signpost_id_t signpostID_ = OS_SIGNPOST_ID_INVALID;
+  std::string_view name_;
+};
